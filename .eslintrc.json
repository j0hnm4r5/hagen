{
	"env": {
		"es2021": true,
		"node": true,
		"browser": true
	},
	"extends": ["airbnb-base", "airbnb-typescript/base", "plugin:prettier/recommended"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"project": "./tsconfig.json"
	},
	"plugins": ["@typescript-eslint", "prettier"],
	"rules": {
		"import/prefer-default-export": "off",
		"no-console": "off",
		"no-plusplus": "off"
	}
}
