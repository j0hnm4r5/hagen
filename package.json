		"test:electron": "electron ./test/electron.js"
	},
	"dependencies": {
		"chalk": "^2.4.1",
		"detect-browser": "^4.1.0"
	},
	"devDependencies": {
		"@j0hnm4r5/eslint-config": "^2.1.6",
